<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#667079">

    <TextView
        android:text="menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:textSize="50sp"
        android:fontFamily="sans-serif"
        android:typeface="sans"
        android:textAlignment="center" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton3"
                android:backgroundTint="#00000000"
                android:src="@drawable/practicegroup" />
        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="1">

        <ImageButton
            android:layout_width="306dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/settingscoggroup"
            android:id="@+id/imageButton4"
            android:background="#00000000" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/analyzeicongroup"
            android:id="@+id/imageButton"
            android:background="#00000000"
            tools:layout_editor_absoluteX="1930dp"
            tools:layout_editor_absoluteY="3650dp" />

    </LinearLayout>

</LinearLayout>